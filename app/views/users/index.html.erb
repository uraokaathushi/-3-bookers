   <% @users.each do |user| %>
  <h2>User info</h2>
  <% if user.profile_image.attached? %>
    <%= image_tag user.profile_image, size: "100x100" %>
  <% else %>
    <%= image_tag 'sample1.jpg', size: "100x100" %>
  <% end %>

  <table>
    <tbody>
      <tr></tr>
      <tr>
        <th>name</th>
        <th><%= user.name %></th>
      </tr>
      <tr>
        <th>introduction</th>
        <th><%= user.introduction %></th>
      </tr>
    </tbody>
  </table>
  <%= link_to "edit", edit_user_path(user) %>
  <% end %>

<h1>New book</h1>
  <%= form_with model: book.new do |f|%>
  <h4>Title</h4>
  <%= f.text_field :title%>
  <h4>Opinion</h4>
  <%= f.text_area :body%>

  <%= f.submit 'Create Book' %>

 <% end %>